<UserControl x:Class="PolyclinicApplication.Views.Components.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PolyclinicApplication.Views.Components"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="120">
    <Grid Height="20" Width="120">
        <PasswordBox Height="20" Width="120" BorderThickness="1" x:Name="PasswordBox" PasswordChanged="PasswordBox_PasswordChanged">
        </PasswordBox>
    </Grid>
</UserControl>
